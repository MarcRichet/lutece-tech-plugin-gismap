<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

    <!-- GIS plugin Informations -->
    <name>gismap</name>
    <class>fr.paris.lutece.plugins.gismap.service.GismapPlugin</class>
    <version>2.0.0-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>gismap.plugin.description</description>
    <provider>gismap.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/gismap/gismap.png</icon-url>
    <copyright>Copyright 2001-2016 Mairie de Paris</copyright>
    <core-version-dependency>
		<min-core-version>2.4.0</min-core-version>
		<max-core-version/>	
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>   

    <!--  Specific plugin CSS stylesheet   --> 
    <css-stylesheets>
          <css-stylesheet>gismap/gismap.css</css-stylesheet> 
    </css-stylesheets>
    
    <admin-features>
        <admin-feature>
            <feature-id>GISMAP_MANAGEMENT</feature-id>
            <feature-title>gismap.adminFeature.gismap_management.name</feature-title>
            <feature-description>gismap.adminFeature.gismap_management.description</feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/gismap/ManageGismap.jsp</feature-url>
            <feature-group>APPLICATIONS</feature-group>
            <feature-icon-url>images/admin/skin/plugins/gismap/gismap_feature.png</feature-icon-url>
        </admin-feature>
    </admin-features>
    
    <!-- Portlet parameters -->
    <portlets>
        <portlet>
            <portlet-class>fr.paris.lutece.plugins.gismap.business.portlet.GismapPortletHome</portlet-class>
            <portlet-type-id>GISMAP_PORTLET</portlet-type-id>
            <portlet-type-name>gismap.portlet.name</portlet-type-name>
            <portlet-creation-url>plugins/gismap/CreatePortletGismap.jsp</portlet-creation-url>
            <portlet-update-url>plugins/gismap/ModifyPortletGismap.jsp</portlet-update-url>
            <portlet-create-action-url>plugins/gismap/DoCreatPortletGismap.jsp</portlet-create-action-url>
            <portlet-modify-action-url>plugins/gismap/DoModifyPortletGismap.jsp</portlet-modify-action-url>
        </portlet>
    </portlets>
    
    <!-- Application XPage  -->
    <!-- Application XPage  -->
    <applications>
       <application>
           <application-id>gismap</application-id>
           <application-class>fr.paris.lutece.plugins.gismap.web.GismapXPageApplication</application-class>
       </application>
    </applications>    
</plug-in>